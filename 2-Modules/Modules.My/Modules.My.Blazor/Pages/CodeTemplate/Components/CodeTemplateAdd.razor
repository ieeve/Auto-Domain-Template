<Form Model="@model"
      LabelColSpan="6"
      OnFinish="SubmitForm"
      WrapperColSpan="18">
    <Row>
        @*CodeGenerator start*@
        <GridCol Span="24">
            <FormItem Label="@L["用户名"]">
                <Input Disabled="!IsAdd" @bind-Value="@context.Id" />
            </FormItem>
            <FormItem Label="@L["说明"]">
                <AntDesign.Input @bind-Value="@context.Notes" />
            </FormItem>
        </GridCol>
        @*CodeGenerator end*@
    </Row>
    <FormItem>
        <LabelTemplate></LabelTemplate>
        <ChildContent>
            <Button Loading="@btnLoading" Disabled="@btnLoading" HtmlType="submit" Type="@ButtonType.Primary">
                @L["确定"]
            </Button>
            <Button OnClick="async () => await ReturnAsync(null)">
                @L["取消"]
            </Button>
        </ChildContent>
    </FormItem>
</Form>
